package graphe;

public abstract class Noeud {
	/* Identifiant du noeud*/ 
	String nom;  
	
	Point position ;
	/**
	 * Construit un nouveau noeud et lui associ
	 * les proprietés relatives à son nom et sa position
	 * @param nom  nom du noeud
	 * @param position position graphique du noeud 
	 */
	Noeud(String nom , Point position){
		this.nom = nom;
		this.position = position;
	}
	
	
	
	void dessiner(Pane zd) {
		
	}

	public Point getPosition() {
		
		return position;
	}
	
	
	
}
